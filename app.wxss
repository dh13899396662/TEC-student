/**app.wxss**/

page {
  background-color: #ededed;
  height: 100%;
  font-family: PingFangSC-Regular;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  box-sizing: border-box;
}

/*隐藏scroll-view滚动条*/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/*距10*/
.ml10{
  margin-left: 10rpx;
}

.mr10{
  margin-right: 10rpx;
}
/*距20*/
.ml20{
  margin-left: 20rpx;
}

.mr20{
  margin-right: 20rpx;
}


/*距30*/
.ml30{
  margin-left: 30rpx;
}

.mr30{
  margin-right: 30rpx;
}

/*距40*/
.ml40{
  margin-left: 40rpx;
}

.mr40{
  margin-right: 40rpx;
}

/*绿色字体*/
.green-color{
  color: #31B17F;
}

/*标题字号*/
.titlefont{
  color: #353535;
}
/* flex布局 */
.flex-row-around{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.flex-row-between{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.flex-column-around{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.flex-column-between{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}


/**
 * 全局颜色
 */
.t-bg {
	background-color: #31B17F;
}

.t-co {
	color: #31B17F;
}

.t-co-orange {
	color: #fe8f36;
}

/**
 * iconfont
 **/
 @font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}


.iconfont {
  font-family: "iconfont" !important;
  font-size: 24rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

