page{
  background-color: white;
}

.success-page{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}

.success-image{
  height: 200rpx;
  width: 200rpx;
  margin-top: 120rpx;
}

.pay-msg{
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 50rpx;
  width: 100%;
  height: 40rpx;
}

.sure-btn{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height: 88rpx;
  width: 650rpx;
  background-color: #31B17F;
  color: white;
  margin-top: 80rpx;
  border-radius: 8rpx;
  font-size: 36rpx;
}

.check-btn{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height: 88rpx;
  width: 650rpx;
  background-color: #FBFAFC;
  color:#353535;
  font-size: 36rpx;
  margin-top: 30rpx;
  border-radius: 8rpx;
  border-width: 2rpx;
  border-color: #D6D5D6;
}